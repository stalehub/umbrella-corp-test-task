    "@parcel/watcher": "^2.4.1",
    "@tailwindcss/typography": "^0.5.12",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.1",
    "@types/lodash.throttle": "^4.1.9",
    "@types/mongoose-unique-validator": "^1.0.9",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
