    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "i18next": "^23.11.2",
    "lodash": "^4.17.21",
    "lodash.throttle": "^4.1.1",
    "mongo-cursor-pagination": "^8.1.3",
    "mongoose-unique-validator": "^5.0.0",
    "react-responsive": "^10.0.0",
    "react-router-dom": "^6.22.3",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.4.5",
    "vite": "^5.2.9",
