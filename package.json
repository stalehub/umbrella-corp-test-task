{
  "name": "umbrella-corp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start-prod": "vite-node ./server/main.ts",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "tailwindcss": "tailwindcss -i ./src/main.css -o ./src/output.css --watch",
    "codegen:compile": "graphql-codegen",
    "codegen:watch": "graphql-codegen -w",
    "cypress:open": "CYPRESS_CACHE_FOLDER=~/cypress_cache cypress open",
    "test:e2e": "CYPRESS_CACHE_FOLDER=~/cypress_cache cypress run"
  },
  "dependencies": {
    "@apollo/client": "^3.10.1",
    "@apollo/server": "^4.10.4",
    "@graphql-tools/schema": "^10.0.3",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "diagnose-endpoint": "^1.1.0",
    "express": "^4.19.2",
    "framer-motion": "^11.2.2",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.2",
    "mongoose-paginate-v2": "^1.8.0",
    "pnpm": "^9.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.0",
    "vite-node": "^1.5.0"
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
